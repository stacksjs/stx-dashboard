{
  "name": "@stacksjs/bun-queue-dashboard",
  "type": "module",
  "version": "0.1.0",
  "description": "A real-time monitoring dashboard for bun-queue, built with stx.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/stx-dashboard#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stx-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stx-dashboard/issues"
  },
  "keywords": [
    "bun-queue",
    "dashboard",
    "monitoring",
    "stx",
    "redis",
    "job-queue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE.md",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun --bun build.ts",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "bun tsc --noEmit"
  },
  "dependencies": {
    "@stacksjs/bun-queue": "^0.0.1"
  },
  "devDependencies": {
    "@stacksjs/stx": "^0.2.5",
    "bun-plugin-stx": "^0.2.5"
  }
}
